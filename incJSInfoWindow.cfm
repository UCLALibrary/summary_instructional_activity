<script language="JavaScript" type="text/javascript">
function newWindow(encoded_url,w,h,x,y,na) {
	self.defaultStatus = "";
	_loc = encoded_url;
	width = w;
	height = h;
	scrnX = x;
	scrnY = y;
	windowName = na;
	window.open(_loc,na,"toolbar=no,width=" + width + "," + "height=" + height + ",screenY=" + scrnY + ",screenX=" + scrnX + ",top=" + scrnY + ",left=" + scrnX + ",scrollbars=yes,resizable=yes");
}
</script>